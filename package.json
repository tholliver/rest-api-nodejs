{
  "name": "nodejs-restapi",
  "version": "1.0.0",
  "description": "Simple Example of nodejs to create rest call using MySQL",
  "nodemonConfig": {
    "watch": [
      "index"
    ],
    "ext": "ts",
    "ignore": [
      "*.test.ts"
    ],
    "delay": "3",
    "execMap": {
      "ts": "ts-node"
    }
  },
  "main": "index.js",
  "engines": {
    "node": "12.18.4",
    "npm": "6.14.6"
  },
  "scripts": {
    "start": "node index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": {
    "name": "Rachel",
    "email": "restapi2example@gmail.com",
    "url": "http://www.resapiexample.com/"
  },
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.16.1",
    "express": "^4.14.1",
    "forever": "^3.0.2",
    "mysql": "^2.13.0",
    "nodemon": "^2.0.6"
  }
}
